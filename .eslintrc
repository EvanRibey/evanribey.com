{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0
  },
  "extends": "airbnb-base",
  "ecmaFeatures": {
    "blockBindings": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}